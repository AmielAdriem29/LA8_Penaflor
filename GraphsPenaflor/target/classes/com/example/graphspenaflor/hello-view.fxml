<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="620.0" prefWidth="730.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.graphspenaflor.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane fx:id="apPane" prefHeight="541.0" prefWidth="690.0">
         <children>
            <StackPane fx:id="circle1" layoutX="350.0" layoutY="130.0" onMouseClicked="#onVertexClicked">
               <children>
                  <Circle fill="#8c08ff" radius="36.0" stroke="BLACK" strokeType="INSIDE" />
                  <Text fx:id="sp2" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="B" textAlignment="CENTER" wrappingWidth="72.0">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Text>
               </children>
            </StackPane>
            <StackPane layoutX="164.0" layoutY="152.0" onMouseClicked="#onVertexClicked">
               <children>
                  <Circle fx:id="sp1" fill="#8c08ff" radius="36.0" stroke="BLACK" strokeType="INSIDE" />
                  <Text fx:id="circle2" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="B" textAlignment="CENTER" wrappingWidth="72.0">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Text>
               </children>
            </StackPane>
         </children>
      </AnchorPane>
      <Button mnemonicParsing="false" onAction="#onSaveClicked" text="SAVE" />
   </children>
</VBox>
